// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Room {
  id          String    @id
  image       String
  name        String 
  current     Int
  total       Int
  tags        String[]
  createdAt   DateTime  @default(now())
}

model User {
  uid          String   @id
  nickname     String
  avatar       String
  createdAt    DateTime @default(now())
}